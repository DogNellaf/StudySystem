<Window x:Class="kursach_klient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kursach_klient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="299">
    <Grid>
        <TextBox x:Name="Login" HorizontalAlignment="Left" Height="22" Margin="99,121,0,0" TextWrapping="Wrap" Text="Enter Your Login" VerticalAlignment="Top" Width="155"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Margin="99,0,0,0" VerticalAlignment="Center" Width="155"/>
        <Button x:Name="Register" Content="Register" HorizontalAlignment="Left" Height="18" Margin="99,197,0,0" VerticalAlignment="Top" Width="65" Click="Register_Click"/>
        <Button x:Name="Enter" Content="Enter" HorizontalAlignment="Left" Height="18" Margin="189,197,0,0" VerticalAlignment="Top" Width="65" Click="Enter_Click"/>
        <Label Content="Login" HorizontalAlignment="Left" Height="26" Margin="10,119,0,0" VerticalAlignment="Top" Width="59"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="26" VerticalAlignment="Center" Width="59" Margin="10,0,0,0"/>

    </Grid>
</Window>
